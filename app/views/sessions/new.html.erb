<h1>Session#new</h1>

<div class="Log_in_form">
  <%= form_tag sessions_path, :method => :post do %>

  <div class="field">
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>
  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <div class="field">
    <%= submit_tag "Login" %>
  </div>
  <% end %>
</div>
